{
  "expo": {
    "name": "BGN⇄EUR Scan",
    "slug": "bgn-eur-scan",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.example.bgneurscan",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.example.bgneurscan",
      "versionCode": 1
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Позволи достъп до камерата за сканиране на цени."
        }
      ]
    ]
  }
}